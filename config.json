{
  "namespace": "screamtest",
  "images": [
    {
      "imageId": "nginxdemos/hello",
      "containerId": "nginx",
      "port": 80,
      "minNumInstances": 1
    }
  ],
  "tests": {
    "healthCheckUrl": "nginx/test",
    "ordered": false,
    "samplers": [
      {
        "method": "GET",
        "targetId": "nginx",
        "path": "/test",
        "percentage": 100
      }
    ]
  },
  "performance": {
    "throughput": 6000,
    "throughputTimeUnit": "MINUTE",
    "latency": 100,
    "failureRate": 0.01
  }
}
